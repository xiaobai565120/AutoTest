[
  {
    "description": "这是一个需要header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
          "content-type": "application/json"
      },
      "json": {
        "name": "wanglaosi",
        "sex": "woman"
      }
    },
    "response": {
      "json": {
        "status": 200,
        "wanglaosi": "success"
      }
    }

  }
]